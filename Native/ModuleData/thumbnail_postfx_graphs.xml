<?xml version="1.0" encoding="utf-8"?>
<base xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	type="particle_system">
	<postfx_graphs>
		<postfx_graph
			id="thumbnail">
			<!-- EXTERNAL LINEAR SCREEN INJECTION -->
			<postfx_node
				id="external_screen_injection"
				shader="postfx_screen_injection"
				format="R11G11B10F"
				size="relative"
				width="1.0"
				height="1.0">
				<output
					index="0"
					type="provided"
					name="screen_rt" />
				<input
					index="0"
					type="provided"
					source="external_screen_input" />
			</postfx_node>
			<postfx_node
				id="halfres_combine"
				class="rglPosftx_halfres_combine_node"
				shader="postfx_half_res_combine"
				format="R16F"
				size="relative"
				width="1"
				height="1">
				<output
					index="0"
					type="provided"
					name="screen_rt" />
				<input
					index="0"
					type="provided"
					source="half_screen_rt" />
				<input
					index="1"
					type="provided"
					source="half_depth_rt" />
				<input
					index="2"
					type="provided"
					source="gbuffer_depth" />
			</postfx_node>
			<postfx_node
				id="histogram"
				class="rglHistogram_node"
				shader="postfx_eye_adaptation"
				format="R16F"
				size="absolute"
				width="1"
				height="1">
				<input
					index="1"
					type="provided"
					source="screen_rt" />
				<input
					index="14"
					type="node"
					source="SSSSS_specular_add" />
				<input
					index="15"
					type="node"
					source="halfres_combine" />
				<!-- just to create a dependency-->
			</postfx_node>
			<postfx_node
				id="motion_vector_normalization"
				shader="postfx_motion_vector_normalizer"
				format="R16G16F"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="provided"
					source="gbuffer_motion_vectors" />
				<preconditions>
					<config
						name="dlss" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="dlss_node"
				shader="postfx_final"
				class="rglDlss_fxnode"
				format="R16G16B16A16F"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="provided"
					source="screen_rt" />
				<input
					index="1"
					type="node"
					source="motion_vector_normalization" />
				<input
					index="2"
					type="provided"
					source="gbuffer_depth" />
				<input
					index="3"
					type="node"
					source="histogram" />
				<preconditions>
					<config
						name="dlss" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="dof_coc"
				shader="postfx_dof_coc"
				format="R16G16F"
				size="relative"
				width="1.0"
				height="1.0">
				<input
					index="0"
					type="provided"
					source="screen_rt"
					exclude="dlss" />
				<input
					index="0"
					type="node"
					source="dlss_node"
					include="dlss" />
				<input
					index="1"
					type="provided"
					source="gbuffer_depth" />
				<!-- For dependency not used in shader -->
				<input
					index="2"
					type="node"
					source="external_screen_injection" />
				<preconditions>
					<config
						name="dof" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="dof_coc_blurx"
				shader="postfx_dof_coc_blurx"
				format="R16G16F"
				size="relative"
				width="1.0"
				height="1.0">
				<input
					index="0"
					type="node"
					source="dof_coc" />
				<preconditions>
					<config
						name="dof" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="dof_coc_blury"
				shader="postfx_dof_coc_blury"
				format="R16G16F"
				size="relative"
				width="1.0"
				height="1.0">
				<input
					index="0"
					type="node"
					source="dof_coc_blurx" />
				<preconditions>
					<config
						name="dof" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="dof_blur_pass_1"
				shader="postfx_dof_blur"
				format="R11G11B10F"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="provided"
					source="screen_rt"
					exclude="dlss" />
				<input
					index="0"
					type="node"
					source="dlss_node"
					include="dlss" />
				<input
					index="1"
					type="node"
					source="dof_coc_blury" />
				<preconditions>
					<config
						name="dof" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="dof_blur_pass_2"
				class="rglDof_blur_final_node"
				shader="postfx_dof_blur"
				format="R11G11B10F"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="node"
					source="dof_blur_pass_1" />
				<input
					index="1"
					type="node"
					source="dof_coc_blury" />
				<preconditions>
					<config
						name="dof" />
				</preconditions>
			</postfx_node>
			<!-- CONTOUR -->
			<postfx_node
				id="contour_prepare"
				shader="postfx_contour_prepare"
				format="R8G8B8A8"
				size="relative"
				width="0.5"
				height="0.5">
				<input
					index="0"
					type="provided"
					source="contour_color" />
				<preconditions>
					<config
						name="contour" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="contour_blurx"
				shader="postfx_contour_blurx"
				format="R8G8B8A8"
				size="relative"
				width="0.5"
				height="0.5">
				<input
					index="0"
					type="node"
					source="contour_prepare" />
				<input
					index="1"
					type="provided"
					source="gbuffer_depth" />
				<preconditions>
					<config
						name="contour" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="contour_blury"
				shader="postfx_contour_blury"
				format="R8G8B8A8"
				size="relative"
				width="0.5"
				height="0.5">
				<input
					index="0"
					type="node"
					source="contour_blurx" />
				<input
					index="1"
					type="provided"
					source="gbuffer_depth" />
				<preconditions>
					<config
						name="contour" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="contour_final"
				class="rglContour_final_fxnode"
				shader="postfx_copy"
				format="R8G8B8A8"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="node"
					source="contour_blury" />
				<preconditions>
					<config
						name="contour" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="contour_combine"
				shader="postfx_contour_combine"
				format="R11G11B10F"
				size="relative"
				width="1"
				height="1">
				<input
					index="1"
					type="node"
					source="dof_blur_pass_2"
					include="dof" />
				<input
					index="1"
					type="provided"
					source="screen_rt"
					exclude="dlss, dof" />
				<input
					index="1"
					type="node"
					source="dlss_node"
					include="dlss"
					exclude="dof" />
				<input
					index="2"
					type="node"
					source="contour_final"
					include="contour" />
				<input
					index="2"
					type="texture"
					source="plain_alpha" />
				<input
					index="3"
					type="node"
					source="plain_alpha" />
				<input
					index="4"
					type="node"
					source="dof_blur_pass_2" />
			</postfx_node>
			<!-- SSSSS -->
			<postfx_node
				id="half_res_velocity_and_depth"
				class="rglHalf_res_velocity_and_depth_node"
				shader="postfx_half_res_velocity_and_depth"
				format="R8G8B8A8"
				size="relative"
				width="0.5"
				height="0.5">
				<input
					index="0"
					type="provided"
					source="gbuffer_depth" />
				<input
					index="1"
					type="provided"
					source="gbuffer_motion_vectors" />
			</postfx_node>
			<!-- NOTE: Filter size is 40 so relative buffer size is 1 / 40 -->
			<postfx_node
				id="tile_max_x"
				shader="postfx_motion_blur_tile_max_x"
				format="R16G16F"
				size="relative"
				width="0.05"
				height="1">
				<input
					index="0"
					type="provided"
					source="gbuffer_motion_vectors" />
				<preconditions>
					<config
						name="motionblur" />
				</preconditions>
