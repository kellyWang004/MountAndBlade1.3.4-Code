					source="contour_combine" />
				<input
					index="11"
					type="node"
					mrt_index="0"
					source="half_res_velocity_and_depth" />
				<input
					index="13"
					type="node"
					mrt_index="1"
					source="half_res_velocity_and_depth" />
				<!-- 4,5,6,7 is taken -->
				<input
					index="12"
					type="node"
					source="histogram" />
				<preconditions>
					<config
						name="temporal_filter" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="sunshaft_blurhalf"
				shader="postfx_sunshafts_blurrhalf"
				format="R16F"
				size="relative"
				width="0.5"
				height="0.5">
				<input
					index="0"
					type="provided"
					source="gbuffer_depth" />
				<preconditions>
					<config
						name="sunshafts" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="sunshafts_rays"
