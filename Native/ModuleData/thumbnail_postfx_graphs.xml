				height="0.05">
				<input
					index="0"
					type="node"
					source="tile_max_y" />
				<preconditions>
					<config
						name="motionblur" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="temporal_smaa_edge_detection"
				class="rglSmaa_edge_detection_node"
				shader="postfx_smaa_edge_detection"
				format="R8G8_UNORM"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="node"
					source="contour_combine" />
				<input
					index="1"
					type="texture"
					source="smaa_areatex" />
				<input
					index="2"
					type="texture"
					source="smaa_searchtex" />
				<input
					index="6"
					type="provided"
					source="gbuffer_depth" />
				<preconditions>
					<config
						name="temporal_smaa" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="temporal_smaa_blend_weight_calc"
				class="rglSmaa_blend_weight_calc_node"
				shader="postfx_smaa_blend_weight_calc"
				format="R8G8B8A8"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="node"
					source="contour_combine" />
				<input
					index="1"
					type="texture"
					source="smaa_areatex" />
				<input
					index="2"
					type="texture"
					source="smaa_searchtex" />
				<input
					index="3"
					type="node"
					source="temporal_smaa_edge_detection" />
				<input
					index="4"
					type="node"
					source="temporal_smaa_edge_detection" />
				<input
					index="6"
					type="provided"
					source="gbuffer_depth" />
				<preconditions>
					<config
						name="temporal_smaa" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="temporal_smaa_blending"
				class="rglHistory_node"
				shader="postfx_smaa_blending"
				format="R11G11B10F"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="node"
					source="contour_combine" />
				<input
					index="1"
					type="texture"
					source="smaa_areatex" />
				<input
					index="2"
					type="texture"
					source="smaa_searchtex" />
				<input
					index="3"
					type="node"
					source="temporal_smaa_edge_detection" />
				<input
					index="4"
					type="node"
					source="temporal_smaa_blend_weight_calc" />
				<input
					index="6"
					type="provided"
					source="gbuffer_depth" />
				<preconditions>
					<config
						name="temporal_smaa" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="taa_resolve_node"
				class="rglTaa_resolve_fxnode"
				shader="postfx_taa_resolve"
				format="R11G11B10F"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="node"
					source="temporal_smaa_blending"
					include="temporal_smaa" />
				<input
					index="0"
					type="node"
					source="contour_combine" />
				<input
					index="1"
					type="provided"
					source="gbuffer_motion_vectors" />
				<input
					index="2"
					type="provided"
					source="gbuffer_depth" />
				<input
					index="3"
					type="node"
					source="contour_combine" />
				<input
					index="11"
					type="node"
					mrt_index="0"
					source="half_res_velocity_and_depth" />
				<input
					index="13"
					type="node"
					mrt_index="1"
					source="half_res_velocity_and_depth" />
				<!-- 4,5,6,7 is taken -->
				<preconditions>
					<config
						name="temporal_resolve" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="taa_filter_node"
				class="rglTaa_filter_fxnode"
				shader="postfx_taa_filter"
				format="R16G16B16A16F"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="node"
					source="taa_resolve_node"
					include="temporal_resolve" />
				<input
					index="0"
					type="node"
					source="temporal_smaa_blending"
					include="temporal_smaa"
					exclude="temporal_resolve" />
				<input
					index="0"
					type="node"
					source="contour_combine" />
				<input
					index="1"
					type="provided"
					source="gbuffer_motion_vectors" />
				<input
					index="2"
					type="provided"
					source="gbuffer_depth" />
				<input
					index="3"
