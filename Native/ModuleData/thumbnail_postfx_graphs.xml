				format="R16F"
				size="relative"
				width="0.25"
				height="0.25">
				<input
					index="0"
					type="node"
					source="sunshaft_blurhalf" />
				<preconditions>
					<config
						name="sunshafts" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="sunshafts_blurt"
				shader="postfx_sunshafts_blurt"
				format="R16F"
				size="relative"
				width="0.25"
				height="0.25">
				<input
					index="0"
					type="node"
					source="sunshafts_rays" />
				<preconditions>
					<config
						name="sunshafts" />
				</preconditions>
			</postfx_node>
			<!-- new bloom -->
			<postfx_node
				id="bright_pass"
				shader="postfx_bloom_threshold"
				format="R11G11B10F"
				size="relative"
				width="1.0"
				height="1.0">
				<input
					index="0"
					type="node"
					source="taa_filter_node"
					include="temporal_filter" />
				<input
					index="0"
					type="node"
					source="taa_resolve_node"
					include="temporal_resolve"
					exclude="temporal_filter" />
				<input
					index="0"
					type="node"
					source="temporal_smaa_blending"
					include="temporal_smaa"
					exclude="temporal_filter, temporal_resolve" />
				<input
					index="0"
					type="node"
					source="contour_combine" />
				<input
					index="4"
					type="node"
					source="histogram" />
				<preconditions>
					<config
						name="hdr" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="motion_blur"
				shader="postfx_motion_blur"
				format="R11G11B10F"
				size="relative"
				width="1"
				height="1">
				<input
					index="0"
					type="node"
					source="taa_filter_node"
					include="temporal_filter" />
				<input
					index="0"
					type="node"
					source="taa_resolve_node"
					include="temporal_resolve"
					exclude="temporal_filter" />
				<input
					index="0"
					type="node"
					source="temporal_smaa_blending"
					include="temporal_smaa"
					exclude="temporal_filter, temporal_resolve" />
				<input
					index="0"
					type="node"
					source="contour_combine" />
				<!--<input index="7" type="provided" source="gbuffer_motion_vectors" /> -->
				<input
					index="11"
					type="node"
					source="tile_neighbor_max" />
				<input
					index="12"
					type="texture"
					source="default_random32" />
				<!--  <input index="13" type="provided" source="gbuffer_depth" /> -->
				<input
					index="13"
					type="node"
					mrt_index="0"
					source="half_res_velocity_and_depth" />
				<input
					index="7"
					type="node"
					mrt_index="1"
					source="half_res_velocity_and_depth" />
				<input
					index="14"
					type="node"
					mrt_index="2"
					source="half_res_velocity_and_depth" />
				<preconditions>
					<config
						name="motionblur" />
				</preconditions>
			</postfx_node>
			<postfx_node
				id="bloom_down1"
				shader="postfx_bloom_down_first"
				format="R11G11B10F"
				size="relative"
				width="0.5"
				height="0.5">
				<input
					index="0"
					type="node"
					source="bright_pass" />
				<preconditions>
					<config
						name="hdr" />
				</preconditions>
			</postfx_node>
			<postfx_node
