			<insertion_definitions>
				<insertion_definition
					label="Add Emitter"
					xml_path="emitters">
					<default_node>
						<emitter
							name="Emitter"
							sample_mesh="prt_mesh_fire_1"
							not_visible_on_low_config="false"
							fadeout_multiplier="1000.000"
							emitter_type="box"
							billboard="3D"
							particles_per_sec="10.000"
							particle_life="2.000"
							gravity="0.000, 0.000, 1.000"
							gravity_weight="10.000"
							turbulance_size="0.000"
							turbulance_strength="0.000"
							starting_scale_time="0.100"
							starting_scale="2.000"
							starting_color_time="0.200"
							starting_color="1.000, 0.235, 0.047"
							starting_alpha="1.000"
							ending_scale_time="0.800"
							ending_scale="0.300"
							ending_color_time="1.000"
							ending_color="0.902, 0.039, 0.039"
							ending_alpha="0.000"
							emit_size="0.200, 0.200, 0.000, 1.000"
							emit_impulse="0.000, 0.000, 1.000"
							emit_randomization="0.000, 0.000, 0.000"
							damping="5.000"
							rotation_speed="1.745"
							rotational_damping="2.000"
							scale_with_velocity_multiplier="0.000"
							scale_with_emitters_velocity="0.000"
							always_emit="true"
							animated_sprite="true"
							sprite_count="25"
							texture_atlas_texture_counts="5.000000, 5.000000"
							sprite_frame_rate="22.000"
							randomize_size="true"
							randomize_rotation="true">
							<childreen />
						</emitter>
					</default_node>
				</insertion_definition>
			</insertion_definitions>
			<items>
				<item
