  <Brush Name="MPHUD.MoraleWidget.Item">
    <Layers>
      <BrushLayer Name="Default" Sprite="MPHud\star_without_glow" AlphaFactor="0.8"/>
    </Layers>
    <Styles>
      <Style Name="Default">
        <BrushLayer Name="Default" />
      </Style>
      <Style Name="IncreaseAnim" AnimationToPlayOnBegin="IncreaseAnimation">
        <BrushLayer Name="Default" />
      </Style>
      <Style Name="IsCriticalAnim" AnimationToPlayOnBegin="IsCriticalAnimation">
        <BrushLayer Name="Default" />
      </Style>
    </Styles>
    <Animations>
      <Animation Name="IncreaseAnimation" Duration="0.75" Loop="true">
        <AnimationProperty LayerName="Default" PropertyName="ColorFactor">
          <KeyFrame Time="0.0" Value="1" />
          <KeyFrame Time="0.075" Value="1.2" />
          <KeyFrame Time="0.125" Value="1.3" />
          <KeyFrame Time="0.2" Value="1.4" />
          <KeyFrame Time="0.25" Value="1.6" />
        </AnimationProperty>
      </Animation>
      <Animation Name="IsCriticalAnimation" Duration="0.75" Loop="true">
        <AnimationProperty LayerName="Default" PropertyName="ColorFactor">
          <KeyFrame Time="0.0" Value="1" />
          <KeyFrame Time="0.05" Value="3.5" />
          <KeyFrame Time="0.375" Value="4" />
        </AnimationProperty>
      </Animation>
    </Animations>
  </Brush>
