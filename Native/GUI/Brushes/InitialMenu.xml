<Brushes>
  <Brush Name="InitialMenuButtonBrush" FontColor="#F4E1C4FF" Font="Galahad" TransitionDuration="0.1">
    <Layers>
      <BrushLayer Name="Default" Color="#FFFFFFFF" />
    </Layers>
    <Styles>
      <Style Name="Default" FontColor="#F4E1C4FF" TextGlowColor="#FFFFFF11" TextGlowRadius="0.1" FontSize="32">
        <BrushLayer Name="Default" FontColor="#F4E1C4FF" />
      </Style>
      <Style Name="Hovered" FontColor="#F3C697FF" TextGlowColor="#F3C69711">
        <BrushLayer Name="Default" FontColor="#F3C697FF" />
      </Style>
      <Style Name="Pressed" FontColor="#B57F5CFF" TextGlowColor="#B57F5C11">
        <BrushLayer Name="Default" />
      </Style>
      <Style Name="Disabled" TextGlowColor="#00000000" TextAlphaFactor="0.35">
        <BrushLayer Name="Default" />
      </Style>
    </Styles>
    <SoundProperties>
      <StateSounds>
      </StateSounds>
      <EventSounds>
        <EventSound EventName="Click" Audio="default" />
      </EventSounds>
    </SoundProperties>
  </Brush>
  <Brush Name="HoverIndicatorBrush" FontColor="#FFFFFFFF" TransitionDuration="0.1">
    <Layers>
      <BrushLayer Name="Default" Sprite="General\InitialMenu\hover_indicator"/>
    </Layers>
    <Styles>
      <Style Name="Default" FontSize="36">
        <BrushLayer Name="Default" AlphaFactor="0" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" AlphaFactor="0"/>
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" AlphaFactor="1" ColorFactor="0.9"/>
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" AlphaFactor="0.2" ColorFactor="0.3" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="HoverIndicatorBrushFlipped" FontColor="#FFFFFFFF" TransitionDuration="0.1">
    <Layers>
      <BrushLayer Name="Default" HorizontalFlip="true" Sprite="General\InitialMenu\hover_indicator"/>
    </Layers>
    <Styles>
      <Style Name="Default" FontSize="36">
        <BrushLayer Name="Default" AlphaFactor="0" />
      </Style>
      <Style Name="Disabled">
        <BrushLayer Name="Default" AlphaFactor="0"/>
      </Style>
      <Style Name="Hovered">
        <BrushLayer Name="Default" AlphaFactor="1" ColorFactor="0.9"/>
      </Style>
      <Style Name="Pressed">
        <BrushLayer Name="Default" AlphaFactor="0.2" ColorFactor="0.3" />
      </Style>
    </Styles>
  </Brush>
  <Brush Name="InitialMenu.Additional.Option" Font="Galahad">
    <Styles>
      <Style Name="Default" FontColor="#F4E1C4FF" TextGlowColor="#000000FF" TextOutlineColor="#000000FF" TextOutlineAmount="0.1" TextGlowRadius="0" TextBlur="0" FontSize="28" />
    </Styles>
  </Brush>
  <Brush Name="InitialMenu.Logo">
    <Layers>
      <BrushLayer Name="Default" Sprite="General\InitialMenu\main_menu_logo" />
    </Layers>
  </Brush>
  <Brush Name="InitialMenu.Logo.WarSails">
    <Layers>
      <BrushLayer Name="Default" Sprite="General\InitialMenu\main_menu_war_sails_logo" Color="#EEE0C7FF" />
    </Layers>
  </Brush>
</Brushes>